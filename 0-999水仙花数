public class Test {
	public static void main(String[] args) {
		int i = 0;
		int tmp = 0;
		//a,b,c 分别代表数字的三个位数
		int a = 0;
		int b = 0;
		int c = 0;
		for(i = 0; i < 1000; i++) {
			tmp = i;//153
			a = tmp % 10;//3
			tmp /= 10;//15
			b = tmp % 10;//5
			tmp /= 10;//1
			c = tmp % 10;//1
			//判断每一位数的三次方相加等于i，打印i
			if(a*a*a + b*b*b + c*c*c == i) {
				System.out.printf("%d ", i);
			}
		}
	}
}
